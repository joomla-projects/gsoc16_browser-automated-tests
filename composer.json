{
    "name": "joomla/joomla-cms",
    "type": "project",
    "description": "Joomla CMS",
    "keywords": ["joomla", "cms"],
    "homepage": "https://github.com/joomla/joomla-cms",
    "license": "GPL-2.0+",
    "config": {
        "platform": {
            "php": "5.5.21"
        },
        "vendor-dir": "libraries/vendor"
    },
    "support": {
        "issues":"https://issues.joomla.org",
        "irc":"http://irc.lc/freenode/joomla/",
        "forum":"http://forum.joomla.org/",
        "docs":"https://docs.joomla.org"
    },
    "require": {
        "php": ">=5.3.10",
        "joomla/application": "~1.5",
        "joomla/di": "~1.2",
        "joomla/event": "~1.1",
        "joomla/registry": "~1.4 >=1.4.5",
        "joomla/session": "~1.3",
        "joomla/string": "~1.3",
        "joomla/uri": "~1.1",
        "joomla/utilities": "~1.3 >=1.3.3",
        "ircmaxell/password-compat": "1.*",
        "leafo/lessphp": "0.5.0",
        "paragonie/random_compat": "~1.0",
        "phpmailer/phpmailer": "5.2.14",
        "symfony/polyfill-php56": "~1.0",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "~1.3",
        "squizlabs/php_codesniffer": "~1.5",
        "pear/cache_lite": "1.7.16",
        "codeception/codeception": "2.2.0-RC2",
        "joomla-projects/joomla-browser": "v3.4.8.3",
        "joomla-projects/selenium-server-standalone": "v2.52.0",
        "codegyre/robo": "~0.6",
        "joomla-projects/robo": "dev-master",
        "fzaninotto/faker": "^1.5",
        "joomla-projects/jorobo": "0.4"
    }
}
